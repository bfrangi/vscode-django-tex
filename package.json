{
    "name": "vscode-django-tex",
    "displayName": "Django-Tex Syntax Highlighting",
    "description": "Syntax Highlighting in Django-Tex Templates",
    "version": "1.0.0",

    "publisher": "bfrangi",
    "license": "SEE LICENSE IN LICENSE.txt",
    "icon": "media/logo.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/bfrangi/vscode-django-tex"
    },
    "bugs": {
        "url": "https://github.com/bfrangi/vscode-django-tex/issues"
    },
    "keywords": [
		"python",
		"django",
		"web",
        "latex"
	],
	"homepage": "https://github.com/bfrangi/vscode-django-tex",
    "engines": {
        "vscode": "^1.69.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "django-tex",
            "aliases": ["Django LaTex", "django-tex"],
            "extensions": [".tex"],
            "configuration": "./language-configuration.json",
            "filenamePatterns": [
                "**/templates/**/*.tex"
            ]
        }],
        "grammars": [{
            "language": "django-tex",
            "scopeName": "text.tex.django",
            "path": "./syntaxes/django-tex.tmLanguage.json"
        }]
    }
}
